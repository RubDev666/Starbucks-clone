{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "build"
            }
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "index.html"
        },
        {
            "src": "/menu",
            "dest": "index.html"
        },
        {
            "src": "/rewards",
            "dest": "index.html"
        },
        {
            "src": "/gift",
            "dest": "index.html"
        }
    ]
}
